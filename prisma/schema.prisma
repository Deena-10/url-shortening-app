datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id          String   @id @default(cuid())
  originalUrl String
  shortCode   String   @unique
  clickCount  Int      @default(0)
  createdAt   DateTime @default(now())
}

